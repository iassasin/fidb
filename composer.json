{
    "name": "iassasin/fidb",
    "description": "Very lightweight and simple database interface for PHP",
    "homepage": "https://github.com/iassasin/fidb",
    "keywords": ["database", "database interface", "db"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "iassasin",
            "email": "iassasin@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "php-coveralls/php-coveralls": "^2"
    },
    "autoload": {
        "psr-4": {
            "Iassasin\\Fidb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iassasin\\Fidb\\Tests\\": "tests"
        }
    }
}
